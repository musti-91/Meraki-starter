{
  "name": "meraki-starter",
  "version": "1.0.0",
  "description": "Meraki-starter: Config react app from scratch",
  "main": "index.js",
  "scripts": {
    "clean-dist": "rm -f -r -d dist",
    "build": "webpack -p --env prod --config=config/prod.js",
    "serve-build": "npm run clean-dist && npm run build && npx http-server dist",
    "start:prod": "npm run clean-dist && npm run build && webpack-dev-server",
    "start-auto": "node src/__test__/AutoTest.js",
    "start-dev": "webpack-dev-server --port 3000 --env dev --config=config/dev.js --open",
    "lint": "lint-staged .",
    "lint:fix": "eslint . --f",
    "test": "jest --coverage --colors",
    "test-watch": "jest --watch",
    "build-coverage": "mv ./coverage/lcov-report ./dist/__coverage",
    "flow-watch": "flow .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "precommit": "npm run test",
    "prepush": "npm run test"
  },
  "author": "Mustafa Alroomi",
  "license": "MIT",
  "lint-staged": {
    "*.{css,scss}": [
      "stylelint"
    ],
    "*.js": [
      "eslint",
      "jest --findRelatedTests"
    ],
    "*.{js,json,css,scss,md,babelrc,eslintrc,prettierrc}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-testcafe": "^0.2.1",
    "file-loader": "^4.0.0",
    "flow-bin": "^0.102.0",
    "flow-typed": "^2.6.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "image-webpack-loader": "^5.0.0",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.0.2",
    "jsdom": "^16.5.0",
    "lint-staged": "^9.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "react-app-polyfill": "^1.0.1",
    "react-polyglot": "^0.4.1",
    "react-spring": "^8.0.27",
    "sass-loader": "^7.1.0",
    "selenium-webdriver": "^4.0.0-alpha.4",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "babel-plugin-emotion": "^10.0.16",
    "eslint": "^4.18.2",
    "node-polyglot": "^2.3.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-spinners": "^0.6.1",
    "redux": "^4.0.3",
    "redux-logger": "^3.0.6"
  },
  "postcss": {},
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>config/test/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
